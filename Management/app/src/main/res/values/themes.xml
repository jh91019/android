<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CafeManagement" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/beige2</item>
        <item name="colorPrimaryVariant">@color/beige1</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="EditTextStyle" parent="Widget.AppCompat.EditText">
        <item name="backgroundTint">@color/beige2</item>
        <item name="android:textSize">18sp</item>
        <item name="android:singleLine">true</item>
        <item name="maxLines">1</item>
        <item name="fontFamily">@font/nanumbarungothic</item>
    </style>

</resources>